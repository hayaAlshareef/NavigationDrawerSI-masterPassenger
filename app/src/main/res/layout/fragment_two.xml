<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:id="@+id/fragmentTwo"
    android:background="@color/gray_shadow">

    <android.support.v7.widget.AppCompatTextView
        android:text="@string/startdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="58dp"
        android:id="@+id/textView8"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="8"
        android:id="@+id/editText"
        android:background="@android:drawable/editbox_background"
        android:layout_alignBottom="@+id/textView8"
        android:layout_toLeftOf="@+id/textView11"
        android:layout_marginRight="21dp"
        android:layout_marginEnd="21dp" />

    <android.support.v7.widget.AppCompatTextView
        android:text="@string/enddate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignEnd="@+id/textView8"
        android:layout_marginTop="23dp"
        android:id="@+id/textView9"
        android:textColor="@color/black" />

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="8"
        android:layout_below="@+id/editText"
        android:layout_alignLeft="@+id/editText"
        android:layout_alignStart="@+id/editText"
        android:layout_marginTop="8dp"
        android:id="@+id/editText2"
        android:background="@android:drawable/editbox_background"
        android:layout_alignBottom="@+id/textView9" />

    <android.support.v7.widget.AppCompatTextView
        android:text="@string/desti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:layout_alignRight="@+id/textView9"
        android:layout_alignEnd="@+id/textView9"
        android:layout_marginTop="53dp"
        android:id="@+id/textView10"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="@string/name"
        android:ems="8"
        android:id="@+id/editText3"
        android:background="@android:drawable/editbox_background"
        android:layout_alignBottom="@+id/floatingActionButton"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_search_category_default"
        android:id="@+id/floatingActionButton"
        app:backgroundTint="@android:color/white"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/editText2"
        android:layout_toLeftOf="@+id/editText3"
        android:layout_toStartOf="@+id/editText3" />

    <android.support.v7.widget.AppCompatTextView
        android:text="@string/location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText3"
        android:layout_alignRight="@+id/textView10"
        android:layout_alignEnd="@+id/textView10"
        android:layout_marginTop="21dp"
        android:id="@+id/textView11"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text="@string/name1"
        android:ems="8"
        android:id="@+id/editText5"
        android:background="@android:drawable/editbox_background"
        android:layout_alignBottom="@+id/textView11"
        android:layout_toRightOf="@+id/floatingActionButton2"
        android:layout_toEndOf="@+id/floatingActionButton2" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_search_category_default"
        android:layout_alignTop="@+id/editText5"
        android:layout_alignRight="@+id/floatingActionButton"
        android:layout_alignEnd="@+id/floatingActionButton"
        android:id="@+id/floatingActionButton2"
        app:backgroundTint="@android:color/white" />

    <RadioButton
        android:text="@string/twoway"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:id="@+id/radioButton"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:buttonTint="@color/black"
        android:layout_below="@+id/floatingActionButton2"
        android:layout_toLeftOf="@+id/textView10"
        android:layout_toStartOf="@+id/textView10" />

    <RadioButton
        android:text="@string/oneway"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/radioButton2"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:layout_alignBaseline="@+id/radioButton"
        android:layout_alignBottom="@+id/radioButton"
        android:layout_toRightOf="@+id/floatingActionButton2"
        android:layout_toEndOf="@+id/floatingActionButton2"
        app:buttonTint="@color/black" />

    <android.support.v7.widget.AppCompatTextView
        android:text="@string/starttime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/radioButton"
        android:layout_alignRight="@+id/textView11"
        android:layout_alignEnd="@+id/textView11"
        android:layout_marginTop="39dp"
        android:id="@+id/textView12"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="8"
        android:id="@+id/editText6"
        android:background="@android:drawable/editbox_background"
        android:layout_alignBaseline="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignLeft="@+id/radioButton2"
        android:layout_alignStart="@+id/radioButton2" />

    <android.support.v7.widget.AppCompatTextView
        android:text="@string/endtime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:layout_alignRight="@+id/textView12"
        android:layout_alignEnd="@+id/textView12"
        android:layout_marginTop="25dp"
        android:id="@+id/textView13"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <android.support.v7.widget.AppCompatEditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="time"
        android:ems="8"
        android:id="@+id/editText7"
        android:background="@android:drawable/editbox_background"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_alignLeft="@+id/editText6"
        android:layout_alignStart="@+id/editText6" />

    <Button
        android:text="@string/searchb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="11dp"
        android:id="@+id/button"
        android:background="@color/blue_button"
        android:layout_alignRight="@+id/textView13"
        android:layout_alignEnd="@+id/textView13"
        android:layout_alignLeft="@+id/floatingActionButton2"
        android:layout_alignStart="@+id/floatingActionButton2"
        android:textSize="24sp" />

</RelativeLayout>